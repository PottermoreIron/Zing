# Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_NAME=member
DB_USERNAME=root
DB_PASSWORD=your_database_password_here

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DATABASE=0

# Nacos Configuration
NACOS_SERVER_ADDR=localhost:8848
NACOS_NAMESPACE=dev

# RabbitMQ Configuration (for framework-starter-mq)
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USERNAME=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_VIRTUAL_HOST=/

# Kafka Configuration (optional, for framework-starter-mq)
KAFKA_BOOTSTRAP_SERVERS=localhost:9092

# JWT Configuration
JWT_PRIVATE_KEY_PATH=classpath:keys/jwt_private_key.pem
JWT_PUBLIC_KEY_PATH=classpath:keys/jwt_public_key.pem

# Application Ports
AUTH_SERVICE_PORT=8081
MEMBER_SERVICE_PORT=11000
GATEWAY_PORT=8080
